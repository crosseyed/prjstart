    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "prjstart --help",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/prjstart",
            "args": ["--help"],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "prjstart search",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/prjstart",
            "args": ["search", "-l", "template1"],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "prjstart start go /tmp/fooo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/prjstart",
            "args": ["start", "go", "/tmp/foo"]
        },
    ]
}
